<div class="container"><div class="row"><h1>New Restaurant</h1>

  <%= simple_form_for(@restaurant) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :phone_number %>
    <%= f.input :category %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
  <% end %></div>
  <br>
  <div class="row">
    <%= link_to 'Back to all restaurants', restaurants_path %></div></div>
